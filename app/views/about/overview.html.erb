<!-- Modal window-->
<div id="myModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h3 id="myModalLabel">Join us!</h3>
  </div>
  <div class="modal-body">
    <p>Primera, I love it…</p>
    <form action="#form" method="post">
      <label for="name"><span>Yor full name</span></label>
      <div class="input fluid-input">
        <input name="name" id="name" type="text" value="name" class="input-xlarge" >
      </div>
      <label for="position"><span>Position</span></label>
      <div class="input fluid-input">
        <input name="position" id="position" type="text" value="position" class="input-xlarge">
      </div>
      <label for="about"><span>About you</span></label>
      <div class="input fluid-input">
        <textarea name="about" id="about" rows="5" class="input-xlarge"></textarea>
      </div>
    </form>
    <h4>Information</h4>
    <p><a href="#" role="button" class="btn popover-test" title="Information" data-content="Add additional informations about your skills.">Read this</a></p>
  </div>
  <div class="modal-footer">
    <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
    <button class="btn btn-primary">Save changes</button>
  </div>
</div>
